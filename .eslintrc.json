{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["prettier"],
  "rules": {
    "import/no-extraneous-dependencies": ["error", { "devDependencies": true }],
    "array-callback-return": "off",
    "consistent-return": "off",
    "import/no-cycle": "off",
    "import/extensions": [
      0,
      {
        "<js>": "always"
      }
    ],
    "no-use-before-define": ["error", { "variables": false }],
    "no-alert": "off",
    "no-console": "off"
  }
}
